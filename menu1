#include<stdio.h>
int main()
{
	int choice;
	printf("choose any no between 1 to 5 \n");
	scanf("%d",&choice);
	switch(choice)
	{
		case 1:printf("your choice is 1, food item:pizza\n price rs.239");
		break;
		case 2:printf("your choice is 2, food item:burger\n price rs.129");
		break;
		case 3:printf("your choice is 3, food item:pasta\n price rs.179");
		break;
		case 4:printf("your choice is 4, food item:french fries\n price rs.99");
		break;
		case 5:printf("your choice is 5, food item:sandwich\n price rs.149");
		break;
		default:printf("you enter wrong no");
	}
	return 0;
}
